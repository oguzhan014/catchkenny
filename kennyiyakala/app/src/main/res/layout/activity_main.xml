<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="367dp"
        android:layout_height="375dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="80dp"
        android:foregroundGravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115">

        <ImageView
            android:id="@+id/kenny1"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny2"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="1"
            app:layout_row="0"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny3"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="2"
            app:layout_row="0"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny4"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="0"
            app:layout_row="1"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny5"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="1"
            app:layout_row="1"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny6"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="2"
            app:layout_row="1"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny7"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="0"
            app:layout_row="2"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny8"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="1"
            app:layout_row="2"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/kenny9"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:onClick="skoruarttir"
            app:layout_column="2"
            app:layout_row="2"
            app:srcCompat="@drawable/kenny2"
            tools:ignore="SpeakableTextPresentCheck" />


    </androidx.gridlayout.widget.GridLayout>

    <TextView
        android:id="@+id/Timetext"
        android:layout_width="102dp"
        android:layout_height="45dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="28dp"
        android:text="Time 15"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Scoretext"
        android:layout_width="94dp"
        android:layout_height="49dp"
        android:layout_marginStart="164dp"
        android:layout_marginBottom="28dp"
        android:text="Score:0"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>